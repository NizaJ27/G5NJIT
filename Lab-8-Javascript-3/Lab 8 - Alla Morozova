        <script>

            let temp1 = document.getElementById('changeTextBtn').addEventListener("click",changeT);
            let temp2 = document.getElementById('displayText');

            function changeT()
            {
                let temp = document.getElementById('displayText');
                temp.innerHTML = "Hello, JavaScript World!";
            }
            let num1 = 5
            let num2 = 15
            console.log(num1+num2)
            console.log(num2-num1)
            console.log(num1*num2)

            let score = 90
            
            function calculateGrade (x)
            {
                if ( x >= 90)
                console.log('A')
                else if (x >=80)
                console.log('B')
                else if ( x >= 70)
                console.log('C')
                else if (x >= 60)
                console.log('D')
                else
                console.log('F')
            }
            calculateGrade(score)

            document.getElementById('myForm').addEventListener('submit', function(event) {
               
                event.preventDefault();
    
                var u_username = document.getElementById('username').value;
                var u_password = document.getElementById('password').value;
               
                if (u_username.trim() === '' || u_password.trim() === '') {
                    alert('Error!');
                } else {
                    
                    setTimeout(function() {
                        alert('Success!');  
                    }, 3000);
                }
            });
        </script>
